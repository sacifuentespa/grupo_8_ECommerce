module.exports = (sequelize, dataTypes) =>{
    let alias = 'Image'
    let cols = {
        idImages:{
            autoIncrement: true,
            primaryKey: true,
            allowNull: false,
            type: dataTypes.INTEGER(18),
        },
        path:{
            allowNull: false,
            unique: true,
            type: dataTypes.STRING(200)
        },
        products_idProduct:{
            
            allowNull: false,
            type: dataTypes.INT(15)
        },
        type:{
            unique: true,
            allowNull: false,
            type: dataTypes.STRING(45)
        }
    }
    let config = {
        tableName: "images",
        timestamps: false
    }
    

    const Image = sequelize.define(alias, cols, config)

    return Image
}